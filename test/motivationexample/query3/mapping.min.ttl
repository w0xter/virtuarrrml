@prefix ns1: <http://semweb.mmlab.be/ns/rml#> .
@prefix ns2: <http://www.w3.org/ns/r2rml#> .

<http://mapping.example.com/map_fruit_0> a ns2:TriplesMap ;
    ns1:logicalSource <http://mapping.example.com/source_1> ;
    ns2:predicateObjectMap <http://mapping.example.com/pom_5> ;
    ns2:subjectMap <http://mapping.example.com/s_1> .

<http://mapping.example.com/jc_0> ns2:child "complementary" ;
    ns2:parent "id" .

<http://mapping.example.com/map_color_0> a ns2:TriplesMap ;
    ns1:logicalSource <http://mapping.example.com/source_0> ;
    ns2:predicateObjectMap <http://mapping.example.com/pom_2> ;
    ns2:subjectMap <http://mapping.example.com/s_0> .

<http://mapping.example.com/om_2> a ns2:ObjectMap ;
    ns2:joinCondition <http://mapping.example.com/jc_0> ;
    ns2:parentTriplesMap <http://mapping.example.com/map_color_0> .

<http://mapping.example.com/om_5> a ns2:ObjectMap ;
    ns1:reference "origin" ;
    ns2:termType ns2:Literal .

<http://mapping.example.com/pm_2> a ns2:PredicateMap ;
    ns2:constant <http://example.com/complementary> .

<http://mapping.example.com/pm_5> a ns2:PredicateMap ;
    ns2:constant <http://example.com/origin> .

<http://mapping.example.com/pom_2> a ns2:PredicateObjectMap ;
    ns2:objectMap <http://mapping.example.com/om_2> ;
    ns2:predicateMap <http://mapping.example.com/pm_2> .

<http://mapping.example.com/pom_5> a ns2:PredicateObjectMap ;
    ns2:objectMap <http://mapping.example.com/om_5> ;
    ns2:predicateMap <http://mapping.example.com/pm_5> .

<http://mapping.example.com/s_0> a ns2:SubjectMap ;
    ns2:template "http://example.com/Color/{name}" .

<http://mapping.example.com/s_1> a ns2:SubjectMap ;
    ns2:template "http://example.com/Fruit/{name}" .

<http://mapping.example.com/source_0> a ns1:LogicalSource ;
    ns1:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
    ns1:source "colors.csv" .

<http://mapping.example.com/source_1> a ns1:LogicalSource ;
    ns1:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
    ns1:source "fruits.csv" .

